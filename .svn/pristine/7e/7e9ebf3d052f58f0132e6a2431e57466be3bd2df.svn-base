.close-icon:before, .close-icon:after {
    background-color: white;
    content: "";
    height: 34px;
    position: absolute;
    right: 22px;
    top: 6px;
    width: 2px;
    z-index: 10;
    }
.close-icon:before {
	transform: rotate(45deg);
	-webkit-transform: rotate(45deg);}
.close-icon:after {
	transform: rotate(-45deg);
	-webkit-transform: rotate(-45deg);}
.close-icon.close-signin{
    color: white;
	cursor: pointer;
	float: right;
	font-size: 20px;
	line-height: 40px;
	margin-top: 10px;
	margin-right: 10px;
	text-align: center;
	width: 40px;
	height:40px;
}
.close-icon.close-signup{
    color: white;
  	cursor: pointer;
	float: right;
	font-size: 20px;
	line-height: 40px;
	margin-top: 10px;
	margin-right: 10px;
	text-align: center;
	width: 40px;
}
