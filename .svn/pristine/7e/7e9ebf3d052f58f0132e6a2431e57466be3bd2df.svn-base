/*Slide Animation*/
.landing-page{
    overflow: hidden;
}
.app-land-cont{
    overflow: hidden;
}

.app-landing-page{
    top:0%;
    left:0%;
}
.signin-form-parent{
    top:0%;
    left:0%;	
    position: absolute;
    transition-duration: 500ms;
    -webkit-transition-duration: 500ms;
    -webkit-transition-timing-function: linear; /* Safari and Chrome */
    transition-timing-function: linear;
    overflow: hidden;
}
.signin-screen-parent{
    top:0%;
    left:0%;
    position: absolute;
    transition-duration: 500ms;
    -webkit-transition-duration: 500ms;
    -webkit-transition-timing-function: linear; /* Safari and Chrome */
    transition-timing-function: linear;
    overflow: auto;
}
.transition-duration-1{
	transition-duration: 500ms;
    -webkit-transition-duration: 500ms;
}
.transition-duration-2{
	transition-duration: 300ms !important;
    -webkit-transition-duration: 300ms !important;
}
.transition-duration-3{
	transition-duration: 00ms !important;
    -webkit-transition-duration: 00ms !important;
}
.slideAnimation-app-landing-page{
	 top:100% !important;
	 
}
.slideAnimation-signin-form-parent-0{
    -webkit-transform:translate3d(0,0,0); /* Safari */
    transform: translate3d(0,0,0);
    z-index:3;
}
.slideAnimation-signin-form-parent--50{
 	-webkit-transform: translate3d(-100%,0,0); /* Safari */
    transform: translate3d(-100%,0,0);
    z-index:2;
}
.slideAnimation-signin-form-parent-50{
    -webkit-transform: translate3d(100%,0,0); /* Safari */
    transform: translate3d(100%,0,0);
    z-index:3;
}

.slideAnimation-signin-form-parent-100{
 	-webkit-transform: translate3d(0,100%,0); /* Safari */
    transform: translate3d(0,100%,0);
    z-index:3;
}   

.slideAnimation-signin-screen-parent-0{
   -webkit-transform:translate3d(0,0,0); /* Safari */
    transform: translate3d(0,0,0);
    z-index:3;
}
.slideAnimation-signin-screen-parent--50{
 	-webkit-transform: translate3d(-100%,0,0); /* Safari */
    transform: translate3d(-100%,0,0);
    z-index:2;
}
.slideAnimation-signin-screen-parent-50{
     -webkit-transform: translate3d(100%,0,0); /* Safari */
    transform: translate3d(100%,0,0);
    z-index:3;
}
.slideAnimation-signin-screen-parent-100{
     -webkit-transform: translate3d(0,100%,0); /* Safari */
    transform: translate3d(0,100%,0);
    z-index:3;
}
