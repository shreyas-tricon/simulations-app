/*******************************************************************************for Profile slideAnimation only*********************************************************/
.profileBuilderModal-inputDetails-animator{
  height: calc(100% - 350px);
  width: 100%;
  top: 350px;
  
  }
 @-webkit-keyframes slideinbottom{
from {
	top : 100%;
	}	

to {
	top : 350px;
	}
}

@keyframes slideinbottom{
from {
	top : 100%;
	}	

to {
	top : 350px;
	}
}
 @-webkit-keyframes slideoutbottom{
from {
	top : 350px;
	}	

to {
	top : 100%;
	}
}

@keyframes slideoutbottom{
from {
	top : 350px;
	}

to {
	top : 100%;
	}
}
.slideinBottom {
	-webkit-animation-name : slideinbottom;
	animation-name : slideinbottom;
}
.slideoutBottom {
	-webkit-animation-name : slideoutbottom;
	animation-name : slideoutbottom;
}

.screenslideinbottom-100px {
	/*-webkit-animation-name : screenslideinbottom-100px;
	animation-name : screenslideinbottom-100px;*/
	-webkit-transform: translateY(-100px);
	transform: translateY(-100px);
	
}
.screenslideinbottom-150px {
	/*-webkit-animation-name : screenslideinbottom-100px;
	animation-name : screenslideinbottom-100px;*/
	-webkit-transform: translateY(-150px);
	transform: translateY(-150px);

}
.screenslideoutbottom-0px {
	/*-webkit-animation-name : screenslideinbottom-100px;
	animation-name : screenslideinbottom-100px;*/
	-webkit-transform: translateY(0);
	transform: translateY(0);
	
}
.screenslideoutbottom-150px {
	/*-webkit-animation-name : screenslideinbottom-100px;
	animation-name : screenslideinbottom-100px;*/
	-webkit-transform: translateY(0);
	transform: translateY(0);
	
}

